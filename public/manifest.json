{
  "name": "libromarko",
  "manifest_version": 2,
  "version": "1.0",

  "description": "Freedom for bookmark.",

  "icons": {
    "512": "logo512.png",
    "192": "logo192.png"
  },

  "browser_action": {
    "default_icon": "logo192.png",
    "default_title": "libromarko",
    "default_popup": "index.html"
  },
  
  "permissions": [
    "storage",
    "tabs"
  ]
}
